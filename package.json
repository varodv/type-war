{
  "name": "type-war",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "vite",
    "lint": "eslint .",
    "lint:ci": "pnpm run lint --cache --cache-strategy content --cache-location ~/.cache/eslint/type-war",
    "build": "vue-tsc --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@vueuse/core": "12.5.0",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@eslint/js": "9.17.0",
    "@storybook/addon-essentials": "8.6.0-alpha.2",
    "@storybook/addon-interactions": "8.6.0-alpha.1",
    "@storybook/blocks": "8.6.0-alpha.1",
    "@storybook/test": "8.6.0-alpha.1",
    "@storybook/vue3": "8.6.0-alpha.1",
    "@storybook/vue3-vite": "8.6.0-alpha.2",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.21.0",
    "@vitejs/plugin-vue": "5.2.0",
    "@vitest/coverage-v8": "2.1.4",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-storybook": "0.11.2",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.1",
    "prettier": "3.4.2",
    "storybook": "8.6.0-alpha.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.0",
    "vite": "5.4.10",
    "vitest": "2.1.4",
    "vue-tsc": "2.2.0"
  },
  "engines": {
    "node": "^20.18",
    "pnpm": "^9"
  }
}
